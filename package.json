{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run nextDev\" \"pnpm run wundergraph\" \"pnpm run database\" \"pnpm run browser\"",
    "wundergraph": "pnpm run dev --filter wundergraph-application",
    "build": "pnpm run build --filter wundergraph-application",
    "browser": "wait-on \"http-get://localhost:3000\" && open-cli http://localhost:3000",
    "database": "docker-compose up",
    "cleanup": "docker-compose down -v",
    "nextDev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@rjsf/core": "^3.2.0",
    "@wundergraph/wunderctl": "^0.64.3",
    "next": "10.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "concurrently": "^6.5.1",
    "open-cli": "^7.0.1",
    "typescript": "^4.5.5"
  }
}