{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:nextDev\" \"npm:wundergraph\" \"npm:database\" \"npm:browser\"",
    "wundergraph": "wait-on \"tcp:54322\" && cd .wundergraph && wunderctl up --debug",
    "browser": "wait-on \"http-get://localhost:3000\" && open-cli http://localhost:3000",
    "database": "docker-compose up",
    "nextDev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@rjsf/core": "^3.2.0",
    "@wundergraph/sdk": "^1.0.0-next.10",
    "graphql": "^15.8.0",
    "next": "10.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "concurrently": "^7.1.0",
    "open-cli": "^7.0.1",
    "typescript": "^4.2.3"
  }
}
